{
  "id": "excalidraw-full",
  "name": "Excalidraw Whiteboard",
  "version": "1.0.0",
  "description": "Full-screen collaborative whiteboard - your new tab is a canvas",
  "author": "DevMe Team",
  "template": "template.html",
  "styles": "styles.css",
  "widgets": {
    "supported": [
      "excalidraw-canvas"
    ],
    "default": [
      "excalidraw-canvas"
    ],
    "positions": {
      "excalidraw-canvas": "canvas-root"
    }
  },
  "cssVariables": {
    "--bg-primary": "#121212",
    "--bg-secondary": "#1e1e1e",
    "--text-primary": "#ffffff",
    "--text-secondary": "#b0b0b0",
    "--primary-color": "#6965db",
    "--accent-color": "#ffffff",
    "--border-color": "rgba(255, 255, 255, 0.2)"
  },
  "fonts": {
    "heading": "Inter",
    "body": "Inter"
  },
  "capabilities": {
    "fullscreen": true,
    "persistence": true,
    "offline": true
  }
}
